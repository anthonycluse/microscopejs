<h2 class="text-error">Delete</h2>
<fieldset>
    <legend>{{- model.name }}</legend>
    
      {{ model.properties.forEach(function(property){ }}
            <label for="{{- property.name }}">{{- property.name }}:</label>
            <p><%- {{- model.name }}.{{- property.name }} %></p>
      {{ }); }}

    <form action="/{{- model.name }}/delete" method="post">
        <input type="hidden" value="<%- {{- model.name }}.id %>" name="id" id="id">
        <p>
            <input type="submit" value="delete" class="btn btn-danger"/>
        </p>
    </form>
</fieldset>

<a class="btn btn-primary" href="/{{- model.name }}">back to list</a>