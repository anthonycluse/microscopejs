<div class="panel panel-default">
  <div class="panel-heading">{{- model.name }} show</div>
  <div class="panel-body">

       {{ model.properties.forEach(function(property){ }}
            <label for="{{- property.name }}">{{- property.name }}:</label>
            <p><%- {{- model.name }}.{{- property.name }} %></p>
        {{ }); }}

  </div>
  <div class="panel-footer">
  		<div class="row">
  			<div class="col-xs-4">
  				<%- link_to('back to list', '/{{- model.name }}', {class : 'btn btn-primary', style : "width:100%;"}) %> 
  			</div>
  			 <div class="col-xs-4">
  				<%- link_to('edit', '/article/edit/'+{{- model.name }}.id, {class : 'btn btn-warning', style : "width:100%;"}) %>
  			</div>
  			<div class="col-xs-4">
  				<%- link_to('delete', '/article/delete/'+{{- model.name }}.id, {class : 'btn btn-danger', style : "width:100%;"}) %>
  			</div>
  		</div>
  </div>
</div>