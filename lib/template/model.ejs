module.exports = function (Sequelize, sequelize) {

	var <%= model.name %> = sequelize.define('<%= model.name %>', {
		<% if (model.properties.length) { %>
			<% model.properties.forEach(function(property){ %>
				<%= property.name %>: Sequelize.<%= property.type %>,
			<% }) %>
		<% } %>
	});
	return <%= model.name %>;
}

